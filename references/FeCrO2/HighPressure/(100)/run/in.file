echo 		    screen
variable	    temp equal 800
variable	    seed equal 534
variable	    seed2 equal 868
variable            timestep equal step

units               real
atom_style          charge
boundary            p p f
atom_modify         map yes

read_data           /share/home/yszhang/${temp}/CrFeO.nvt
molecule            O2 /share/home/yszhang/O2.mol 

pair_style          reaxff NULL
pair_coeff          * * /share/home/yszhang/potentials/CrOFeSCH.reax Cr Fe O

######################## fix bot ####################
region		    r_fixed_crfe block INF INF INF INF INF 4.32
group		    g_fixed_crfe region r_fixed_crfe

#######################  nvt CrFe ####################
region		    r_nvt_crfe block INF INF INF INF 4.32 27.31

########################## nve CrFe + O ###################
